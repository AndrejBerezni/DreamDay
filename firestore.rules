rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /Chapters/{chapter} {
      allow read,
      write: if request.auth != null;
    }
    match /Users/{user} {
      allow read,
      write: if request.auth != null;
    }
    match /Tasks/{task} {
      allow read,
      write: if request.auth != null;
    }
    }
  }